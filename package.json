    "url": "https://github.com/Youssef-Harby/ol-esri-style/issues"
  },
  "homepage": "https://github.com/Youssef-Harby/ol-esri-style#readme",
  "dependencies": {
    "ol": "^9.1.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/register": "^7.23.7",
    "chai": "^5.1.0",
    "esm": "^3.2.25",
    "mocha": "^10.4.0",
    "parcel": "^2.12.0"
